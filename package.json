{
  "name": "notfound",
  "version": "1.0.0",
  "description": "a 404 page generator",
  "author": "Ezekiel Mangalu <emangalu@jirnexu.com>",
  "private": true,
  "scripts": {
    "dev": "parcel index.html",
    "start": "npm run dev",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "test": "npm run unit && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build": "parcel build index.html"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  },
  "dependencies": {
    "normalize.css": "^8.0.0",
    "vue": "^2.5.2",
    "vue-hot-reload-api": "^2.3.0",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^1.3.1",
    "babel-plugin-dynamic-import": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "node": "^10.3.0",
    "parcel-bundler": "^1.8.1",
    "vue-template-compiler": "^2.5.16"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}

{
  "name": "notfound",
  "version": "1.0.0",
  "description": "a 404 page generator",
  "author": "Ezekiel Mangalu <emangalu@jirnexu.com>",
  "private": true,
  "scripts": {
    "dev": "concurrently 'parcel watch index.html' 'nodemon server.js'",
    "start": "npm run dev",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "test": "npm run unit && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build": "parcel build index.html"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "normalize.css": "^8.0.0",
    "vue": "^2.5.2",
    "vue-hot-reload-api": "^2.3.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^1.3.1",
    "@vue/test-utils": "^1.0.0-beta.16",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^3.5.1",
    "connect-history-api-fallback": "^1.5.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jest": "^23.1.0",
    "jest-serializer-vue": "^2.0.2",
    "mongoose": "^5.1.4",
    "node": "^10.3.0",
    "nodemon": "^1.17.5",
    "parcel-bundler": "^1.8.1",
    "vue-jest": "^2.6.0",
    "vue-template-compiler": "^2.5.16"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
